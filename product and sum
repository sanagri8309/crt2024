import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int a,b;
        a=s.nextInt();
        b=s.nextInt();
        int c=a*b;
        if(c>1000)
        {
            c=a+b;
            System.out.println(c);
        }
        else
        {
            c=a*b;
         System.out.println(c);
        }
    }
}
