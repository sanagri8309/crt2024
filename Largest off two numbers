import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        long A,B;
        A=s.nextLong();
        B=s.nextLong();
        if(A>B)
            System.out.println(A);
        else
            System.out.println(B);
        
    }
}
